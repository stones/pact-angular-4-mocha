  "author": "Tom Stones <tom@tomstones.com.au>",
  "scripts": {
    "clean": "rimraf dll compiled dist",
    "mocha": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "mocha:watch": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts --watch",
    "mocha:report": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=mochawesome --reporter-options reportDir=reports/unit/",
    "start": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch  --hot --content-base src/",
    "pact:test": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.pact.opts",
    "pact:publish": "node config/pact.publish.js",
    "test": "npm run mocha",
    "test:watch": "npm run mocha:watch",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
